"scripts": {
   "UNCOMMENT_IF_pre-install-cmd": [
         "-d memory_limit=-1 composer self-update --2"
    ], 
    "post-root-package-install": [
        "php-legal-licenses generate --hide-version"
    ],
    "post-create-project-cmd": [
        "php-legal-licenses generate --hide-version"
    ],
    "post-install-cmd": [
        "php-legal-licenses generate --hide-version"
    ],
    "post-update-cmd": [
        "php-legal-licenses generate --hide-version"
    ]
},
"extra" : {
	 "compile-mode": "all",
	 "webfan" : {
		 "install" : {
		     
		 }
	 }, 
        "downloads": {
          "vendor-patch/autoload-package": {
            "url": "https://github.com/vendor-patch/autoload-package/archive/refs/heads/master.zip",
            "path": "./modules/vendor-patch/autoload-package",
            "ignore": [".*"]
        }
    }
},
"minimum-stability":"dev",
    "config": {
 
        "cache-files-ttl": 900
    },  
	 "require-dev" : {		 

	 },
  "require" : {
	"php": ">=8.0",
	"frdl/webfan" : "dev-master", 	  
	"composer/composer" :"*",		
	"vendor-patch/autoload-package" :"*",	
	"friendsofphp/proxy-manager-lts":"*",     
	"yosymfony/resource-watcher" : "*",			 
	"sensiolabs/ansi-to-html" : "*",
        "teqneers/shamir" : "*",		
	"frdl/php-floodprotection": "*",
	"frdl/remote-psr4" : "*",
	"frdl/proxy" : "*",
	"webfan3/php-easy-breaker" : "*",
        "comcast/php-legal-licenses":"*",
	"ninsuo/php-shared-memory":"*", 
	"nette/utils":"*", 
	"nette/caching" :"*", 
	"nette/robot-loader":"*",	
	"nette/reflection":"*",  
	"nette/php-generator":"*",
	"spatie/temporary-directory": "*",
	"nikic/php-parser" : "*",
	"hellogerard/jobby" : "*",
	"php-di/php-di" : "*",
	"caseyamcl/configula" : "*"
  },	  
  "autoload": {
    "classmap" : ["src/"],
    "psr-4": {

     }
  },
  "suggest" : {

  }
}
