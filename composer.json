{
	"name": "frdl/codebase",
	"description": "Codebase API related packages...",
	"license": ["MIT"],
	"homepage": "https://webfan.de/install/",
	"authors": [{
		"name": "Till Wehowski",
		"email": "software@frdl.de"
	}],

	"scripts": {
		"UNCOMMENT_IF_pre-install-cmd": [
			"-d memory_limit=-1 composer self-update --2"
		],
		"post-root-package-install": [
			"php-legal-licenses generate --hide-version"
		],
		"post-create-project-cmd": [
			"php-legal-licenses generate --hide-version"
		],
		"post-install-cmd": [
			"php-legal-licenses generate --hide-version"
		],
		"post-update-cmd": [
			"php-legal-licenses generate --hide-version"
		]
	},
	"extra": {

		"compile-mode": "all",
		"foxy": true,
		"installer-types": [
			"frdl-extension",
			"frdl-theme",
			"frdl-module",
			"frdl-httpdocs",
			"frdl-public",
			"webfan-package",
			"webfan-extension",
			"webfan-theme",
			"webfan-module",
			"webfan-httpdocs",
			"webfan-public",
			"vendor",
			"npm-package",
			"node-module",
			"project",
			"automad-package"
		],
		"installer-paths": {
			"packages\/{$vendor}\/{$name}\/": [
				"type:automad-package"
			],
			"modules\/frdl\/{$vendor}.{$name}\/": [
				"type:frdl-module"
			],
			"modules\/webfan\/{$vendor}\/{$name}\/": [
				"type:frdl-module"
			],
			"modules\/packages\/{$vendor}\/{$name}\/": [
				"type:frdl-package"
			],

			"modules\/extensions\/{$name}\/{$vendor}~{$name}\/": [
				"type:frdl-extension"
			],
			"modules\/themes\/{$name}\/{$vendor}~{$name}\/": [
				"type:frdl-theme"
			],
			"webfantized\/{$name}\/{$vendor}\/": [
				"type:webfan-feature"
			],
			"config\/{$name}\/{$vendor}\/": [
				"type:frdl-config"
			],
			"modules\/service\/{$name}\/{$vendor}\/": [
				"type:frdl-service"
			],
			"~/.webfan/shared\/{$name}\/{$vendor}\/": [
				"type:frdl-shared"
			],
			"userland\/": [
				"type:webfan-userland"
			],
			"{$webroot}\/": [
				"type:webfan-webfiles"
			],
			"public\/": [
				"type:frdl-httpdocs"
			],
			"vendor\/frdlweb\/{$vendor}\/{$name}\/": [
				"vendor:frdl",
				"vendor:frdlweb",
				"vendor:webfan",
				"vendor:webfan3"
			],
			"./": [
				"type:frdl-patch"
			],
			"{$workspace}\/node_modules\/{$vendor}\/{$name}\/": [
				"type:node-module"
			],
			"node_modules\/{$vendor}.{$name}\/": [
				"type:npm-package"
			],
			"..\/{$vendor}~{$name}~project\/": [
				"type:project"
			]
		},
		"dependency-scripts": {
			"run": true,
			"trust": [
				"airmad\/*",
				"symfony",
				"symfony\/symfony",
				"symfony\/console",
				"frdl\/console",
				"frdl\/*",
				"webfan3\/*",
				"wehowski\/*"
			],
			"exclude": [],
			"types": [
				"frdl-extension",
				"frdl-theme",
				"frdl-module",
				"frdl-shared",
				"frdl-public",
				"webfan-extension",
				"webfan-theme",
				"webfan-module",
				"webfan-package",
				"frdl-service",
				"frdl-config",
				"webfan-feature",
				"webfan-webfiles",
				"webfan-userland",
				"frdl-patch",
				"automad-package"
			]
		},
		"config-plugin-output-dir": "config",

		"webfan": {
			"install": {

			}
		},
		"downloads": {
			"*": {
				"path": "./../../../vendor/{$id}",
				"ignore": ["test", "tests", "doc", "docs", ".*"]
			},
			"frdl/array-helper": {
				"url": "https://github.com/frdl/array-helper/archive/refs/heads/main.zip",
				"path": "./../../../vendor/frdl/array-helper",
				"ignore": [".*"]
			},
			"frdl/proxy": {
				"url": "https://github.com/frdl/proxy/archive/refs/heads/master.zip",
				"path": "./../../../vendor/frdl/proxy",
				"ignore": [".*"]
			},
			"vendor-patch/psecio.secure_dotenv": {
				"url": "https://github.com/vendor-patch/secure_dotenv/archive/refs/heads/master.zip",
				"path": "./../../../vendor/vendor-patch/psecio.secure_dotenv",
				"ignore": [".*"]
			},
			"inverisoss/tiny-php-queue": {
				"url": "https://github.com/inverisOSS/tiny-php-queue/archive/refs/tags/1.0.0.zip",
				"path": "./../../../vendor/inverisoss/tiny-php-queue",
				"ignore": [".*"]
			},
			"nelexa/zip": {
				"url": "https://github.com/Ne-Lexa/php-zip/archive/refs/tags/4.0.1.zip",
				"path": "./../../../vendor/nelexa/zip",
				"ignore": [".*"]
			},
			"jenssegers/proxy": {
				"url": "https://github.com/jenssegers/php-proxy/archive/refs/tags/v3.1.0.zip",
				"path": "./../../../vendor/jenssegers/proxy",
				"ignore": [".*"]
			}
		}
	},
	"minimum-stability": "dev",
	"config": {
		"bin-dir": "./bin",
		"preferred-install": "auto",
		"cache-files-ttl": 3600,
		"prepend-autoloader": true,
		"allow-plugins": {
			"airmad/*": true,
			"civicrm/*": true,
			"composer/*": true,
			"frdl/*": true,
			"webfan3/*": true,
			"kylekatarnls/update-helper": true
		},
		"frdl": {
			"webfantize": [],
			"level": "level-*"
		},
		"webfan-website": {
			"webroot": "public",
			"root": "."
		}
	},
	"require-dev": {

	},
	"require": {
		"php": ">=8.0",
		"frdl/webfan": "*",
		"composer/composer": "*",
		"friendsofphp/proxy-manager-lts": "*",
		"yosymfony/resource-watcher": "*",
		"sensiolabs/ansi-to-html": "*",

		"frdl/php-floodprotection": "*",
		"frdl/remote-psr4": "*",
		"frdl/event-module": "*",
		"webfan3/php-easy-breaker": ">=0.3.6",
		"comcast/php-legal-licenses": "*",
		"ninsuo/php-shared-memory": "*",
		"nette/utils": "*",
		"nette/caching": "*",
		"nette/robot-loader": "*",
		"nette/reflection": "*",
		"nette/php-generator": ">=4.0.6",
		"spatie/temporary-directory": "*",
		"nikic/php-parser": "*",
		"hellogerard/jobby": "*",

		"caseyamcl/configula": "*",
		"karelwintersky/monolog-pdo-handler": "*",
		"catfan/medoo": "*",
		"byjg/migration": "*",
		"bramus/router": "*",
		"adhocore/cli": "*",
		"rosell-dk/locate-binaries": "*",
		"rosell-dk/htaccess-capability-tester": "*",
		"rosell-dk/exec-with-fallback": "*",

		"symfony/finder": "*",
		"psr/http-message": "*",

		"laminas/laminas-httphandlerrunner": ">=2.5.0",

		"roave/security-advisories": "dev-master",
		"frdl/auth": ">=0.0.3",
		"frdl/cms": ">=0.0.2",
		"rosell-dk/image-mime-type-guesser": "^0.3",
		"league/commonmark": "*",
		"mustangostang/spyc": "^0.6",
		"minicli/minicli": "*",
		"league/flysystem-sftp": "*",
		"league/flysystem-webdav": "*",
		"league/flysystem-ziparchive": "*",
		"slim/slim": "*",
		"vlucas/phpdotenv": "*",
		"php-vfs/php-vfs": "*",
		"brzuchal/filesystem-stream-wrapper": "*",
		"ivopetkov/html5-dom-document-php": "*",
		"ivopetkov/html-server-components-compiler": ">=1.1.0",
		"fusio/sdk": "*",
		"altorouter/altorouter": "*",
		"league/html-to-markdown": "*",
		"league/pipeline": "*",
		"commonjs/commonjs": "*",
		"robthree/twofactorauth": "*",
		"guzzlehttp/psr7": "*",
		"phpactor/path-finder": "*",
		"uma/json-rpc": "*",
		"phpseclib/phpseclib": "*",
		"m2mtech/flysystem-stream-wrapper": "*",
		"php-di/invoker": "*",
		"mirazmac/php-requirements-checker": "*",
		"mirazmac/dotenvwriter": "*",
		"league/route": ">=5.1.2",
		"wmde/php-vuejs-templating": "*",
		"php-di/php-di": "6.4.0",
		"ocramius/proxy-manager": "*",
		"spatie/once": "*",
		"roave/better-reflection": "*",
		"appstract/php-hostsfile": "*",
		"joomla/keychain": "~2.0",
		"landrok/activitypub": "*",
		"delirehberi/webfinger": "*",
		"relay/relay": "~2.0",
		"workerman/phpsocket.io": "*",
		"teqneers/shamir": "*",
		"blake-gardner/mac-address": "*",
		"ggergo/enum": "*"
	},

	"autoload": {
		"classmap": ["src/"],
		"psr-4": {

		},
		"files": ["functions.php"]
	},
	"suggest": {
		"frdl/project": "Frdlweb project template",
		"webfan3/website": "Default Start/Installer Template",
		"frdlweb/webfat": "...if you want to build websites or load the webfat stub helper classes... (this could produce issues on codebase server!)"
	}
}
